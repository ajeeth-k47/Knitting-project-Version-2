<div id="viewtab">

     <h3>Enter <%= name%> Info</h3>

     <br />

     <form method="POST" action="/api/v1/master/<%= name %>">
       <div class="row">

         <div class="col-lg-3">
          <label for="name"><%= name%> Name</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="text" name="name" value="" required />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">Address Line#1</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="text" name="address1" value="" required />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">Address Line#2</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="text" name="address2" value="" required />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">Country</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control"  type="text" name="country" value="" required />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">State</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="text" name="state" value="" required />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">City</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="text" name="city" value="" required />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">Pin Code</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="number" name="pin" value="" required />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">Primary Phone</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="text" name="phone" value="" />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">Mobile No</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control"  type=number name="mobile" value="" required />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">Primary Email</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="text" name="email" value="" />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">Website URL</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="text" name="website" value="" />
         </div>

       </div>
         <br />

       <div class="row">

         <div class="col-lg-3">
          <label for="name">GSTIN</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="text" name="gst" value="" />
         </div>

       </div>
       <br />
       <div class="row">

         <div class="col-lg-3">
          <label for="name">PAN Card</label>
         </div>
         <div class="col-lg-7">
          <input class="form-control" type="text" name="pan" value="" />
         </div>

       </div>

       <br />

       <div class="row">

          <div class="col-lg-3">

          </div>
          <div class="col-lg-7">
           <input type="submit" class="btn btn-lg btn-info" name="" value="Save" />
          </div>

       </div>
     </form>

</div>
